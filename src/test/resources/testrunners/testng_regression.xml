<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="OpenCart App Test Regression Suite" verbose="4" thread-count="4" parallel="tests">

	<listeners>
		<listener class-name="com.qa.opencart.listeners.ExtentReportListener" />
		<listener class-name="com.qa.opencart.listeners.TestAllureListener" />
		<listener class-name="com.qa.opencart.listeners.AnnotationTransformer" />
	</listeners>

	<test name="LoginPage Test_chrome_90">
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="browserversion" value="90.0"></parameter>
	<parameter name="testcasename" value="Login Page Test"></parameter>
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="LoginPage Test_chrome_109">
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="browserversion" value="109.0"></parameter>
	<parameter name="testcasename" value="Login Page Test"></parameter>
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>
	<test name="AccountsPage Test_chrome_79">
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="browserversion" value="79.0"></parameter>
	<parameter name="testcasename" value="Account Page Test"></parameter>
		<classes>
			<class name="com.qa.opencart.tests.AccountsPageTest" />
		</classes>
	</test>
	
	
